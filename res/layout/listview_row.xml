<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rowTextView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    android:orientation="horizontal">
    <TextView android:layout_alignParentLeft="true" android:layout_width="100dp" android:layout_height="match_parent" android:id="@+id/person_name"/>
    <TextView android:layout_alignParentRight="true" android:layout_width="wrap_content" android:layout_height="match_parent" android:id="@+id/person_time"/>
</RelativeLayout>
